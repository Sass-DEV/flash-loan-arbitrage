{
  "name": "flash-loan-arbitrage-dashboard",
  "version": "2.1.0",
  "description": "Professional DeFi arbitrage platform with flash loan integration",
  "main": "index.js",
  "scripts": {
    "start": "http-server -p 8080",
    "dev": "webpack-dev-server --open",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,css,json}\"",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "docs": "jsdoc -c jsdoc.json",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "defi",
    "flash-loan",
    "arbitrage",
    "ethereum",
    "blockchain",
    "trading",
    "mev",
    "dex",
    "uniswap",
    "sushiswap",
    "aave"
  ],
  "author": "DeFi Builder",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.0",
    "apexcharts": "^3.44.0",
    "ethers": "^6.8.0",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.0.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/flash-loan-arbitrage.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/flash-loan-arbitrage/issues"
  },
  "homepage": "https://yourusername.github.io/flash-loan-arbitrage"
}
